{
    "version": 2,
    "builds": [
      {
        "src": "public/**/*",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "handle": "filesystem"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }
  